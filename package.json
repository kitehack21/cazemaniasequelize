  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development nodemon index.js ",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "undo-migration": "node_modules/.bin/sequelize db:migrate:undo",
    "deep-undo-migration": "node_modules/.bin/sequelize db:migrate:undo:all",
    "server": "cross-env NODE_ENV=production node_modules/.bin/forever start --killTree index.js",
    "list-server": "node_modules/.bin/forever list",
    "log-server": "node_modules/.bin/forever logs",
    "restart-server": "node_modules/.bin/forever restartall",
    "stop-server": "node_modules/.bin/forever stopall && taskkill /f /im node.exe"
  },
